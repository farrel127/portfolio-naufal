{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/install.php", "dest": "/api/install.php" },
    { "src": "/admin.php", "dest": "/api/admin.php" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
